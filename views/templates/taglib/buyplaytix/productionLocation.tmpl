<nst:root xmlns:nst="http://nest.sourceforge.net/"
	xmlns:c="urn:nsttl:HTML_Template_Nest_Taglib_Standard">

		<div class="mapLink">
					<div class="vcard">
					<p class="fn">
						${location->NAME}
					</p>
					<p class="adr">
						<span class="street-address">${location->ADDRESS1} <c:if
								test="location->ADDRESS2">
								<br />${location->ADDRESS2}</c:if></span><br /> <span class="city">${location->CITY}</span>,
						<span class="region">${location->REGION}</span> <span
							class="postal-code">${location->POSTALCODE}</span><br />
					</p>
					<p>
					<a target="_mapWindow"
              href="https://maps.google.com/?q=${location->ADDRESS1},${location->ADDRESS2},${location->CITY},${location->REGION} ${location->POSTALCODE}"
              title="map to ${location->NAME}">Map <img
              src="https://secure.buyplaytix.com/images/map.gif" /></a>
          </p>
				</div>

		</div>
</nst:root>
